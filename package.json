  "author": "karlodini.kozic <karlodini.kozic@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build && npm run kill-port",
    "start": "node dist/index.js",
    "kill-port": "lsof -ti :3001 | xargs kill -9 || true",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\"  \"nodemon dist/index.js\""
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.32.1",
